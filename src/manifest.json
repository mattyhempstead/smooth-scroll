{
    "name": "Smooth Scroll",
    "description": "Adds shortcuts to smoothly scroll with the keyboard.",
    "version": "0.2.0",
    "manifest_version": 3,
    "icons": {
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
   "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
           "js": [
                "js/content.js"
            ],
            "run_at" : "document_end"
        }
    ]
}
