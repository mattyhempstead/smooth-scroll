{
    "name": "Smooth Scroll",
    "description": "A Chrome extension to allow for quick and easy keyboard scrolling",
    "version": "0.1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
           "js": [
                "js/content.js"
            ],
            "run_at" : "document_end"
        }
    ]
}
